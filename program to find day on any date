dn=int(input("Enter the day number"))
dy=input("Enter the first day")

dc={1:"Monday",2:"Tuesday",3:"Wednesday",4:"Thursday",5:"Friday",6:"Saturday",7:"Sunday"}

if dy=="Monday":
    if dn%7==0:
        d=dc[7]
    elif dn%7==1:
        d=dc[1]
    elif dn%7==2:
        d=dc[2]
    elif dn%7==3:
        d=dc[3]
    elif dn%7==4:
        d=dc[4]
    elif dn%7==5:
        d=dc[5]
    elif dn%7==6:
        d=dc[6]

if dy=="Tuesday":
    if dn%7==0:
        d=dc[1]
    elif dn%7==1:
        d=dc[2]
    elif dn%7==2:
        d=dc[3]
    elif dn%7==3:
        d=dc[4]
    elif dn%7==4:
        d=dc[5]
    elif dn%7==5:
        d=dc[6]
    elif dn%7==6:
        d=dc[7]
if dy=="Wednesday":
    if dn%7==0:
        d=dc[2]
    elif dn%7==1:
        d=dc[3]
    elif dn%7==2:
        d=dc[4]
    elif dn%7==3:
        d=dc[5]
    elif dn%7==4:
        d=dc[6]
    elif dn%7==5:
        d=dc[7]
    elif dn%7==6:
        d=dc[1]
        
if dy=="Thursday":
    if dn%7==0:
        d=dc[3]
    elif dn%7==1:
        d=dc[4]
    elif dn%7==2:
        d=dc[5]
    elif dn%7==3:
        d=dc[6]
    elif dn%7==4:
        d=dc[7]
    elif dn%7==5:
        d=dc[1]
    elif dn%7==6:
        d=dc[2]
        
if dy=="Friday":
    if dn%7==0:
        d=dc[4]
    elif dn%7==1:
        d=dc[5]
    elif dn%7==2:
        d=dc[6]
    elif dn%7==3:
        d=dc[7]
    elif dn%7==4:
        d=dc[1]
    elif dn%7==5:
        d=dc[2]
    elif dn%7==6:
        d=dc[3]
        
if dy=="Saturday":
    if dn%7==0:
        d=dc[5]
    elif dn%7==1:
        d=dc[6]
    elif dn%7==2:
        d=dc[7]
    elif dn%7==3:
        d=dc[1]
    elif dn%7==4:
        d=dc[2]
    elif dn%7==5:
        d=dc[3]
    elif dn%7==6:
        d=dc[4]
        
if dy=="Sunday":
    if dn%7==0:
        d=dc[6]
    elif dn%7==1:
        d=dc[7]
    elif dn%7==2:
        d=dc[1]
    elif dn%7==3:
        d=dc[2]
    elif dn%7==4:
        d=dc[3]
    elif dn%7==5:
        d=dc[4]
    elif dn%7==6:
        d=dc[5]
print(d)
